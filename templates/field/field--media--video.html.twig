{#
/**
 * @file
 * Theme override for the Video media type.
 *
 */
#}

{% if multiple %}
  <div{{ attributes }}>
    {% for item in items %}
      <div{{ item.attributes }}>{{ item.content }}</div>
    {% endfor %}
  </div>
{% else %}
  {% for item in items %}
    <div{{ attributes.addClass(field_name) }}>{{ item.content }}</div>
  {% endfor %}
{% endif %}
