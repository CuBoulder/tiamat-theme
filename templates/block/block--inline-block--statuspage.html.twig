{{ attach_library('boulderD9_base/ucb-status-page') }}

{%
  set classes = [
  'block',
  'block-' ~ configuration.provider|clean_class,
  'block-' ~ plugin_id|clean_class,
  bundle ? 'block--type-' ~ bundle|clean_class,
  view_mode ? 'block--view-mode-' ~ view_mode|clean_class
]
%}

	<div class="container">
		<h1{{title_attributes}}>
			{{ label }}
		</h1>
        <status-page url="{{content.field_ucb_status_page_url.0['#url']}}"></status-page>
	</div>
